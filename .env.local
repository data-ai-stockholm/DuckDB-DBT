# Local Development Environment
# Use this for testing without AWS, Azure, GCS, or external Polaris

# Storage: Use local filesystem (no cloud dependencies)
STORAGE_BACKEND=local

# Table format: Use Iceberg
TABLE_FORMAT=iceberg

# Iceberg Catalog: Use local filesystem (no external catalog service needed)
ICEBERG_CATALOG_TYPE=local
ICEBERG_WAREHOUSE=warehouse/

# Database path
DATABASE_PATH=weather_reports.db

# API Configuration (optional)
WEATHER_API_TOKEN=
